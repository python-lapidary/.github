{% extends 'module.py.jinja2' %}
{% block body -%}
{% set model = model.body -%}
class {{ model.class_name }}({{ model.base_type.__repr__() }}):
{%- if model.docstr %}
    """
    {{ model.docstr | indent }}
    """
{% endif -%}
{% if model.base_type.__repr__() == 'pydantic.BaseModel' %}
{% include 'schema_class_object.py.jinja2' %}
{% elif model.base_type.__repr__() == 'enum.Enum' %}
{% include 'schema_class_enum.py.jinja2' %}
{% endif %}
{%- endblock %}
